  "dependencies": {
    "bootstrap": "^4.6.2",
    "bootstrap-icons": "^1.9.1",
    "data": "^1.0.0",
    "datatables.net": "^1.12.1",
    "datatables.net-bs4": "^1.12.1",
    "datatables.net-buttons": "^2.2.3",
    "datatables.net-buttons-bs4": "^2.2.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "jquery": "^3.6.1",
    "normalize.css": "^8.0.1",
    "semver": "^7.3.8"
  },
  "tap": {
    "nyc-arg": [
      "--exclude",
      "tap-snapshots/**"
    ]
