{
  "name": "www",
  "version": "1.0.0",
  "private": true,
  "main": "lib/data/index.js",
  "scripts": {
    "dev": "node ./bin/build.mjs --dev",
    "build": "node ./bin/build.mjs --prod",
    "clean": "rm -rf lib/data/daily && rm -rf lib/data/latest.json",
    "test": "node --test './test/**/*.js'",
    "lint": "npm run eslint",
    "postlint": "template-oss-check",
    "template-oss-apply": "template-oss-apply --force",
    "lintfix": "npm run eslint -- --fix",
    "snap": "node --test --test-update-snapshots './test/**/*.js'",
    "posttest": "npm run lint",
    "eslint": "eslint \"**/*.{js,cjs,ts,mjs,jsx,tsx}\"",
    "test:cover": "node --test --experimental-test-coverage --test-timeout=3000 --test-coverage-lines=80 --test-coverage-functions=80 --test-coverage-branches=80 './test/**/*.js'",
    "test:node20": "node --test test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/npm/statusboard.git",
    "directory": "workspaces/www"
  },
  "keywords": [],
  "author": "GitHub Inc.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/npm/statusboard/issues"
  },
  "homepage": "https://github.com/npm/statusboard#readme",
  "devDependencies": {
    "@npmcli/eslint-config": "^6.0.0",
    "@npmcli/template-oss": "4.29.0",
    "eslint-import-resolver-typescript": "^4.3.1"
  },
  "files": [
    "bin/",
    "lib/"
  ],
  "engines": {
    "node": "^18.17.0 || >=20.5.0"
  },
  "templateOSS": {
    "//@npmcli/template-oss": "This file is partially managed by @npmcli/template-oss. Edits may be overwritten.",
    "version": "4.29.0",
    "content": "../../scripts/template-oss",
    "testRunner": "node:test",
    "coverageThreshold": 80
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.9.1",
    "data": "^1.0.0",
    "datatables.net": "^2.0.7",
    "datatables.net-bs4": "^2.0.7",
    "datatables.net-buttons": "^3.0.2",
    "datatables.net-buttons-bs4": "^3.0.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.0",
    "esbuild": "^0.27.0",
    "http-server": "^14.1.1",
    "jquery": "^3.7.1",
    "normalize.css": "^8.0.1",
    "semver": "^7.3.8"
  }
}
